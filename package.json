{
  "name": "react-headless-formatter",
  "version": "1.0.0",
  "description": "Transform HTML-like strings into fully rendered React components",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && scripts/build.sh",
    "dev": "tsc --w --sourceMap",
    "clean": "rimraf dist",
    "test": "npm run test-only && npm run lint && npm run typecheck",
    "lint": "eslint --ext .ts --ext .tsx src/",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "test-only": "cross-env NODE_ENV=test jest",
    "test-dev": "cross-env NODE_ENV=test node --inspect node_modules/jest/bin/jest.js --watch --runInBand",
    "typecheck": "tsc --noEmit -p ./tsconfig.json",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danikaze/react-headless-formatter.git"
  },
  "private": true,
  "keywords": [
    "typescript",
    "npm",
    "lib",
    "boilerplate",
    "library",
    "react",
    "html",
    "parser",
    "tokenizer",
    "render",
    "component",
    "headless",
    "ui"
  ],
  "author": "danikaze <danikaze@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danikaze/react-headless-formatter/issues"
  },
  "homepage": "https://github.com/danikaze/react-headless-formatter#readme",
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "devDependencies": {
    "@alex_neo/jest-expect-message": "^1.0.5",
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@ef-carbon/tspm": "^2.2.5",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/jest-expect-message": "^1.1.0",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "babel-jest": "^29.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {}
}
